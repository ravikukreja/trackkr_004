<h1>Listing graphs</h1>
<table>
  <tr>
    <th>Category Id</th>
    <th>Name</th>
    <th>Image</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @graphs.each do |graph| %>
  <tr>
    <td><%= graph.t_category_id %></td>
    <td><%= graph.name %></td>
    <td><%= graph.image %></td>
     <td><%= graph.description %></td>
    <td><%= link_to 'Show', graph %></td>
    <td><%= link_to 'Edit', edit_graph_path(graph) %></td>
    <td><%= link_to 'Destroy', graph, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Graph', new_graph_path %>
