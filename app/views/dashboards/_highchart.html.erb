

<script type="text/javascript" charset="utf-8">
  $(function() {
    new Highcharts.Chart({
      chart: {
        renderTo: "user_chart",
defaultSeriesType: 'line'
      },
      title: {
        text: "Distance by Day"
      },
	  
   
	  
	  credits: {
		text: "trackkr ",
		href: "www.trackkr.com"
	   },
      xAxis: {
        type: "datetime"
      },
      yAxis: {
        title: {
          text: "Distance"
        }
      },
      series: [{
	  	name: "Actual",
		pointInterval: <%= 1.day * 1000 %>,
		pointStart: <%= 2.weeks.ago.to_i * 1000 %>,  
	  	data: <%= (2.weeks.ago.to_date..Date.today).map {|date| UserProductPlanData.user_actual_distance(date, @user_product_plan_id)}.inspect%>
		}, {
	  	name: "Planned",
		pointInterval: <%= 1.day * 1000 %>,
		pointStart: <%= 2.weeks.ago.to_i * 1000 %>,  
		data: <%= (2.weeks.ago.to_date..Date.today).map {|date| UserProductPlanData.user_plan_distance(date, @user_product_plan_id)}.inspect%>,
		<%= link_to (date.day), "#{new_user_product_plan_data_path}?date=#{date}",:rel => "facebox[.bolder]" %>
		}]
       });
	  
		
		
	  }) 
	   
  });
  
</script> 


