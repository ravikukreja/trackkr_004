

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
<title>trackkr-track your key results</title>
<style type="text/css">
<!--
body {
	margin-left: 0px;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
}
.white{
	background-color:#fff;
}
.red{
	background-color:red
}
.green{
	background-color:008000;
}
-->
</style>

   

<% @user_product_plan_graphs.each do |user_product_plan_graph| %>

	<p>User graphs ID:<%= user_product_plan_graph.graph_id%></p>
	<p>User graphs name:<%= user_product_plan_graph.graph.name%></p>
	
	<% if  user_product_plan_graph.graph_id == 1 %>
	<%= render 'dashboards/highchart1'%>
	<% elsif user_product_plan_graph.graph_id == 2 %>
	<%= render 'dashboards/highchart2'%>
	<% elsif user_product_plan_graph.graph_id == 3 %>
	<%= render 'dashboards/highchart3'%>
	<% elsif user_product_plan_graph.graph_id == 4 %>
	<%= render 'dashboards/highchart4'%>
	<% elsif user_product_plan_graph.graph_id == 5 %>
	<%= render 'dashboards/highchart5'%>
	<%end%>
<% end %>

</head>

<body>
	
</div>	


<div> Pending Friends List on this product:</div>
<ul>
	
	<% for friendship in current_user.friendships%>
	<li>
	<%= friendship.friend.username%>
	<%= link_to "Remove", friendship, :method => :delete %>
		</li>
	<%end%>
</ul>

 

<div>  <strong>List of Plans on this product:</strong></div>
<ul>
	<% @user_product_plans.each do |user_product|%>
		<li>
			<%= link_to user_product.product_plan.product_plan_name, dashboards_path(:user_product_plan_id => user_product.id) %>
			<%= user_product.id%>
			
		</li>
	<%end%>
	
	<%= button_to "Add new Plan", trackkr_modules_path, :method => :get%>
	
</ul>

<div style="height:300px;width:950px;font:12px; overflow:auto;">
<%= render 'user_product_plan_datas/edit_individual'%>	
</div>
<%= button_to "Home", trackkr_modules_path, :method => :get %>	
<%= button_to "Edit Chart Setting", new_graph_plan_path(:user_product_plan_id=>session[:user_product_plan_id]), :method => :get%>
<div id="user_chart"></div>
<div style="height:100px;width:300px; overflow:auto;">	
  <p><strong>GOAL:</strong></p>
	<ul>
		
		<% @user_product_plan_ids.each do |plan_detail|%>
		 <p> Product Plan ID: <%= plan_detail.id %></p> 
		 <p> Start Date: 	  <%= plan_detail.start_date%></p>	
		 <p> End Date: 	  	  <%= plan_detail.end_date%></p>
		 <%= button_to 'Destroy', plan_detail, :confirm => 'Are you sure? If you delete the product plan all your data will be lost', :method => :delete %>	
		<%end%>
		 	
	</ul> 			
</div> 		
<div id="common">

<div class="regionhdr"> Calendar
<div id="calendar">
	<h2 id="month">
		<%= link_to "<", :month => (@date.beginning_of_month-1).strftime("%Y-%m"), :user_product_plan_id => session[:user_product_plan_id]%>
		<%= h @date.strftime("%B %Y")%>
		<%= link_to ">", :month => (@date.end_of_month+1).strftime("%Y-%m"), :user_product_plan_id => session[:user_product_plan_id]%>
	</h2> 
<%= calendar_for @user_product_plan_datas, :year => @date.year, :month => @date.month do |calendar| %>
	<%= calendar.head('Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat') %>
	<%= calendar.day(:day_method => :training_date) do |date, user_product_plan_datas|%>
	<%data = user_product_plan_datas_row(session[:user_product_plan_id] , date)%>
    <div class="<%=get_color(data)%>">
    	<%= link_to (date.day), "#{new_user_product_plan_data_path}?date=#{date}",:rel => "facebox[.bolder]" %>
	</div>

<% end %>
</div>
 <%= link_to "Big Calendar", user_product_plan_datas_path, :method => :get  %>
	<%end%>
</div>

</body>
</html>


