

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
<title>trackkr-track your key results</title>
<style type="text/css">
<!--
body {
	margin-left: 0px;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
}
-->
</style>


<%= render 'dashboards/highchart'%>

</head>

<body>

<div id ="frd_network"> <%=link_to "Add Friend to Network",new_friend_path(:product_id=>Product.first.id),{:rel=>"facebox[.bolder]"}%></div>

<div> Approved Friends List on this product:</div>
<ul>
	<%@active_friends.each do |frd|%>
		<li>
			<%=frd.username%>
		</li>
	<%end%>
</ul>



<div>  List of Plans on this product:</div>
<ul>
	<% @user_product_plans.each do | user_product|%>
		<li>
			<%= link_to user_product.product_plan.product_plan_name, dashboards_path(:product_plan_id => user_product.product_plan_id) %>
			<%= user_product.id%>
			
		</li>
	<%end%>
</ul>


<%= render 'user_product_plan_datas/edit_individual'%>	
<%= button_to "Home", trackkr_modules_path, :method => :get %>	
<%= button_to "Edit Chart Setting", new_user_product_plan_graph_path, :method => :get%>
<div id="user_chart"></div>

<div id="common">

<div class="regionhdr"> Calendar
<div id="calendar">
	<h2 id="month">
		<%= link_to "<", :month => (@date.beginning_of_month-1).strftime("%Y-%m")%>
		<%= h @date.strftime("%B %Y")%>
		<%= link_to ">", :month => (@date.end_of_month+1).strftime("%Y-%m")%>
	</h2> 
<%= calendar_for @user_product_plan_datas, :year => @date.year, :month => @date.month do |calendar| %>
	<%= calendar.head('Sun', 'Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat') %>
	<%= calendar.day(:day_method => :training_date) do |date, user_product_plan_datas|%>
		<%= link_to (date.day), "#{new_user_product_plan_data_path}?date=#{date}",:rel => "facebox[.bolder]" %>
			<ul>
				<% for user_product_plan_data in user_product_plan_datas%>
					<%= user_product_plan_data.plan_distance %>
					<%= user_product_plan_data.actual_distance %>
				<% end %>
			</ul>
	<% end %>
<% end %>
</div>

</div>

</body>
</html>


